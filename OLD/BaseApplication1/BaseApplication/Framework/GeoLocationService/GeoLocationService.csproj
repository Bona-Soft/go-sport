<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyVersion>2.0.0.0</AssemblyVersion>
    <FileVersion>2.0.0.0</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>2.0.0</Version>
    <Authors>Malcolm Jack</Authors>
    <Company>Wibci Labs</Company>
    <Product>Offline Country Reverse Geocode</Product>
    <Description>given latitude and longitude, get the country or american state name where the point is located. Offline operation - no api calls necessary</Description>
    <PackageProjectUrl>https://github.com/InquisitorJax/Wibci.CountryReverseGeocode</PackageProjectUrl>
    <PackageIconUrl>http://wibcisoft.com/Images/Wibcisoft.png</PackageIconUrl>
    <RepositoryUrl>https://github.com/InquisitorJax/Wibci.CountryReverseGeocode</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Xamarin, .NET, Geolocation</PackageTags>
    <PackageReleaseNotes>.NET Standard update</PackageReleaseNotes>
    <AssemblyName>myb.Framework.GeoLocationService</AssemblyName>
    <RootNamespace>myb.Framework.GeoLocationService</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <OutputPath>..\..\..\Build\debug\</OutputPath>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="GeolocationService.cs" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>


</Project>
