<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="Default.aspx.cs" Inherits="TestApp.Default" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<title></title>
</head>
<body>
	<form id="form1" name="form1" runat="server" action="Default.aspx">
		<div>
			<table>
				<tr>
				</tr>
				<tr>
					<td>Username:
                        <input type="text" value="" name="LoginName" />
					</td>
					<td>Password:
                        <input type="password" value="" name="LoginPassword" />
					</td>
				</tr>
				<tr>
					<td>
						<input type="submit" value="login" />
					</td>
					<td>
						<input type="button" value="logout" onclick="document.getElementById('form1').action = 'Default.aspx?logout=1'; document.getElementById('form1').submit(); " />
					</td>
				</tr>
			</table>
		</div>
	</form>
</body>
</html>