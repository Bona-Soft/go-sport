/***********************************
MYB Tags
************************************/
.myb-tag-container {
	border-bottom: 1px solid #eee;
	min-height: 10px;
	margin-bottom: 10px;
	padding-bottom: 10px;
}

	.myb-tag-container:after, .myb-tag-container:before {
		content: " ";
		display: table;
	}

	.myb-tag-container:after {
		clear: both;
	}

	.myb-tag-container .myb-tag-items {
		padding-top: 7px;
	}

	.myb-tag-container .myb-tag-tools {
		float: right;
		display: inline-block;
		font-size: 25px;
	}

.myb-tag {
	background-image: none;
	display: inline-block;
	margin-bottom: 0;
	font-weight: 400;
	text-align: center;
	vertical-align: middle;
	touch-action: manipulation;
	cursor: pointer;
	border: 1px solid transparent;
	white-space: nowrap;
	padding: 6px 12px;
	font-size: 14px;
	line-height: 1.42857;
	border-radius: 4px;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
	box-shadow: none !important;
	outline: 0;
	margin: 0 3px 3px 0;
}

	.myb-tag.active, .myb-tag:active {
		outline: 0;
		-webkit-box-shadow: inset 0 3px 5px rgba(0,0,0,.125);
		box-shadow: inset 0 3px 5px rgba(0,0,0,.125);
		background-image: none;
	}

		.myb-tag.active.focus, .myb-tag.active:focus, .myb-tag.focus,
		.myb-tag:active.focus, .myb-tag:active:focus, .myb-tag:focus {
			outline: dotted thin;
			outline: -webkit-focus-ring-color auto 5px;
			outline-offset: -2px;
		}

	.myb-tag.focus, .myb-tag:focus, .myb-tag:hover {
		color: #333;
		text-decoration: none;
	}

	.myb-tag:hover {
		transition: all .3s;
	}

.myb-tag-default {
	color: #333;
	background-color: #fff;
	border-color: #ccc;
}

	.myb-tag-default.active, .myb-tag-default:active, .myb-tag-default:hover {
		color: #333;
		background-color: #e6e6e6;
		border-color: #adadad;
		background-image: none;
	}

	.myb-tag-default.focus, .myb-tag-default:focus {
		color: #333;
		background-color: #e6e6e6;
		border-color: #8c8c8c;
	}

.myb-tag-lg {
	padding: 10px 16px;
	font-size: 18px;
	line-height: 1.33333;
	border-radius: 6px;
}

.myb-tag-sm {
	padding: 5px 10px;
	font-size: 12px;
	line-height: 1.5;
	border-radius: 3px;
}

.myb-tag-xs {
	padding: 1px 5px;
	font-size: 12px;
	line-height: 1.5;
	border-radius: 3px;
}

.myb-tag:not(.myb-tag-sm):not(.myb-tag-lg) {
	line-height: 1.44;
}

.myb-tag .close {
	margin-top: 5px !important;
	margin-left: 5px !important;
}

