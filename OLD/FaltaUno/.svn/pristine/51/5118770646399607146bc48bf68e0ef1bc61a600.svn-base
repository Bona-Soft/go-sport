}

/*Used to hide default browser input*/
.myb-control-input {
	position: absolute;
	z-index: -1;
	opacity: 0;
}

/*Custom checkbox indicator*/
.myb-checkbox-indicator {
	position: absolute;
	top: .1em;
	left: 0;
	display: block;
	width: 18px;
	height: 18px;
	pointer-events: none;
	-webkit-user-select: none;
