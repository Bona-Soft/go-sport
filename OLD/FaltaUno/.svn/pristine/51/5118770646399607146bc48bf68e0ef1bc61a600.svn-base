	display: none;
}

	.carousel.slick-initialized {
		display: block;
	}

.base-slick .slick-prev:before, .slick-next:before {
	color: #ddd;
}

/*******************
Scroll to Top
********************/
.scroll-to-top {
	display: inline-block;
	/*padding: 1px;*/
	text-align: center;
	position: fixed;
	bottom: 50px;
	z-index: 10001;
	display: none;
	right: 10px;
}

	.scroll-to-top > i {
		display: inline-block;
		color: #687991;
		font-size: 30px;
		opacity: 0.6;
		filter: alpha(opacity=60);
	}

	.scroll-to-top:hover {
		cursor: pointer;
	}

		.scroll-to-top:hover > i {
			opacity: 1;
			filter: alpha(opacity=100);
		}

@media (min-width: 992px) {
	/* 992px */
	.scroll-to-top {
		right: 20px;
	}

	.header-search {
		max-width: 600px;
	}
}

@media (max-width: 991px) {
	/* 991px */
	.scroll-to-top {
		bottom: 50px;
		right: 10px;
	}

	.scroll-to-top > i {
		font-size: 28px;
	}

	.header-search {
		max-width: 400px;
	}
}